<?xml version="1.0" encoding="UTF-8"?>
<!-- 本文档为智联之窗（WsnBox）APP（通用版）提供所有默认配置信息 -->
<!-- 所有时间单位若无特别指出，均默认为毫秒 -->
<configuration>
	<settings>
		<communicators>
			<communicator name="UDP">
				<enable>false</enable>
				<ip>192.168.1.18</ip>
				<port>5000</port>
				<DataRequestCycle>500</DataRequestCycle>
			</communicator>
			<communicator name="TCP">
				<enable>false</enable>
				<ip>122.225.88.90</ip>
				<port>33361</port>
				<DataRequestCycle>2000</DataRequestCycle>
			</communicator>
			<communicator name="BLE">
				<enable>true</enable>
				<!-- 单位秒 -->
				<ScanCycle>5</ScanCycle>
				<!-- 单位秒 -->
				<ScanDuration>10</ScanDuration>
			</communicator>
			<communicator name="SerialPort">
				<enable>false</enable>
				<PortName>/dev/ttyHSL1</PortName>
				<BaudRate>115200</BaudRate>
				<DataRequestCycle>2000</DataRequestCycle>
			</communicator>
			<communicator name="USB">
				<enable>false</enable>
                <vpid>0</vpid>
				<BaudRate>115200</BaudRate>
                <DataBits>8</DataBits>
                <StopBits>1</StopBits>
                <parity>0</parity>
				<protocol>ESB</protocol>
				<DataRequestCycle>2000</DataRequestCycle>
			</communicator>
		</communicators>
		<DataProcessor>
			<enable>true</enable>
			<!-- 单位秒 -->
			<GatherCycle>60</GatherCycle>
		</DataProcessor>
	</settings>
</configuration>
