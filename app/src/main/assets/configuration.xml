<?xml version="1.0" encoding="UTF-8"?>
<!-- 本文档为智联之窗（WsnBox）APP（通用版）提供所有默认配置信息 -->
<!-- 所有时间单位若无特别指出，均默认为毫秒 -->
<configuration>
	<settings>
		<communicators>
			<communicator name="UDP">
				<enable>true</enable>
				<ip>192.168.1.18</ip>
				<port>5000</port>
				<DataRequestCycle>500</DataRequestCycle>
			</communicator>
			<communicator name="BLE">
				<enable>true</enable>
				<ScanCycle>5000</ScanCycle>
				<ScanDuration>10000</ScanDuration>
			</communicator>
			<communicator name="SerialPort">
				<enable>false</enable>
				<PortName>ttyHSL1</PortName>
				<BaudRate>115200</BaudRate>
				<DataRequestCycle>2000</DataRequestCycle>
			</communicator>
		</communicators>
		<DataProcessor>
			<enable>true</enable>
			<GatherCycle>60000</GatherCycle>
		</DataProcessor>
	</settings>
</configuration>
