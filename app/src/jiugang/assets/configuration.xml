<!-- 本文档为智联之窗（WsnBox）APP（通用版）提供所有默认配置信息 -->
<!-- 所有时间单位若无特别指出，均默认为毫秒 -->
<configuration>
	<settings>
		<communicators>
			<communicator name="TCP">
				<enable>true</enable>
				<ip>122.225.88.90</ip>
				<port>5001</port>
				<DataRequestCycle>2000</DataRequestCycle>
			</communicator>
		</communicators>
		<DataProcessor>
			<enable>true</enable>
			<!-- 单位秒 -->
			<GatherCycle>60</GatherCycle>
		</DataProcessor>
	</settings>
</configuration>
