<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <SwitchPreference
        android:key="@string/preference_key_data_warn_enable"
        android:title="@string/preference_title_data_warn_enable"
        android:switchTextOn="@string/enable"
        android:switchTextOff="@string/disable"/>
    <SwitchPreference
        android:key="@string/preference_key_data_warn_notify_enable"
        android:title="@string/preference_title_data_warn_notify_enable"
        android:switchTextOn="@string/enable"
        android:switchTextOff="@string/disable"/>
    <PreferenceCategory
        android:key="@string/preference_key_notification_parameter"
        android:title="@string/preference_title_notification_parameter">
        <SwitchPreference
            android:key="@string/preference_key_data_warn_notify_sound_enable"
            android:title="@string/preference_title_data_warn_notify_sound_enable"
            android:switchTextOn="@string/enable"
            android:switchTextOff="@string/disable"/>
        <SwitchPreference
            android:key="@string/preference_key_data_warn_notify_vibrate_enable"
            android:title="@string/preference_title_data_warn_notify_vibrate_enable"
            android:switchTextOn="@string/enable"
            android:switchTextOff="@string/disable"/>
        <SwitchPreference
            android:key="@string/preference_key_data_warn_notify_float_enable"
            android:title="@string/preference_title_data_warn_notify_float_enable"
            android:switchTextOn="@string/enable"
            android:switchTextOff="@string/disable"/>
        <SwitchPreference
            android:key="@string/preference_key_data_warn_notify_screen_enable"
            android:title="@string/preference_title_data_warn_notify_screen_enable"
            android:switchTextOn="@string/enable"
            android:switchTextOff="@string/disable"/>
        <Preference
            android:key="@string/preference_key_data_warn_notify_settings"
            android:title="@string/preference_title_data_warn_notify_settings"
            android:summary="@string/preference_summary_data_warn_notify_settings"/>
    </PreferenceCategory>
    <SwitchPreference
        android:key="@string/preference_key_data_warn_toast_enable"
        android:title="@string/preference_title_data_warn_toast_enable"
        android:switchTextOn="@string/enable"
        android:switchTextOff="@string/disable"/>
    <SwitchPreference
        android:key="@string/preference_key_data_monitor_background_enable"
        android:title="@string/preference_title_data_monitor_background_enable"
        android:summary="@string/preference_summary_data_monitor_background_enable"
        android:switchTextOn="@string/enable"
        android:switchTextOff="@string/disable"/>
</PreferenceScreen>